  </servlet>
  <servlet>
  	<servlet-name>BrandController</servlet-name>
  	<display-name>BrandController</display-name>
  	<description></description>
  	<servlet-class>com.bumbalbee.controller.BrandController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CategoryController</servlet-name>
  	<display-name>CategoryController</display-name>
  	<description></description>
  	<servlet-class>com.bumbalbee.controller.CategoryController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CategoryController1</servlet-name>
  	<display-name>CategoryController1</display-name>
  	<description></description>
  	<servlet-class>com.bumbalbee.controller.CategoryController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CustomerController</servlet-name>
  	<display-name>CustomerController</display-name>
