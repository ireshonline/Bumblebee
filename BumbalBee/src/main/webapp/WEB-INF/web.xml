  	<display-name>SalesController</display-name>
  	<description></description>
  	<servlet-class>com.bumbalbee.controller.SalesController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SalesContoller</servlet-name>
  	<display-name>SalesContoller</display-name>
  	<description></description>
  	<servlet-class>com.bumbalbee.controller.SalesContoller</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>admincontroller</servlet-name>
  	<display-name>admincontroller</display-name>
  	<description></description>
  	<servlet-class>com.bumbalbee.controller.AdminController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ProductController</servlet-name>
  	<url-pattern>/product_controller</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
  	<servlet-name>ProductController</servlet-name>
  	<url-pattern>/allproducts</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>BrandController</servlet-name>
  	<url-pattern>/brandcontroller</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CategoryController</servlet-name>
  	<url-pattern>/CategoryController</url-pattern>
