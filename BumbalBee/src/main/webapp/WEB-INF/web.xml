  	<url-pattern>/categorycontroller</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CustomerController</servlet-name>
  	<url-pattern>/customercontroller</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SalesController</servlet-name>
  	<url-pattern>/salescontroller</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SalesContoller</servlet-name>
  	<url-pattern>/salescontoller</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>admincontroller</servlet-name>
  	<url-pattern>/admincontroller</url-pattern>
